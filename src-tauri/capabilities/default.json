{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-open",
    {
      "identifier": "opener:allow-open-url",
      "allow": [{"url": "*"}]
    },
    {
      "identifier": "opener:allow-default-urls",
      "allow": [{"url": "*"}]
    }
  ]
}
